{
    "$schema": "./appsettings.schema.json",
    "NLog": {
        "throwConfigExceptions": true,
        "targets": {
            "trace": {
                "type": "Trace"
            }
        },
        "rules": [
            {
                "logger": "*",
                "minLevel": "Trace",
                "writeTo": "trace"
            }
        ]
    }
}